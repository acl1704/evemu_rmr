<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="XMLPKT Generator"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="XMLPkt"
			DisplayName="XML Packet Generator"
			CommandLine="$(OutDir)xmlpktgen.exe [inputs]"
			Outputs="$(InputName).h"
			AdditionalDependencies="$(OutDir)xmlpktgen.exe"
			FileExtensions="*.xmlp"
			ExecutionDescription="Generating packet processors from XML: $(InputName)"
			ShowOnlyRuleProperties="false"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
